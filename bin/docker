#!/usr/bin/env node

const util = require("util");

const translate = require("../lib/translate");

const deploymentName = process.env.SCAFFOLD_DEPLOYMENT_NAME;
const originalPath = process.env.SCAFFOLD_ORIGINAL_PATH;
const opts = {deploymentName: deploymentName};

translate(opts, process.argv).execute().catch((err) => {
  console.error(err);
  process.exit(1);
});
